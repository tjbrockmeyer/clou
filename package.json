{
  "name": "clou",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env CLIMB_DEBUG=1 ts-node src/cli",
    "test": "jest",
    "build": "tsc",
    "package": "nexe dist/cli/index.js -n clou -o target -b -r 'schemas/*.json' -r 'infra/aws/templates/*.yml' --verbose",
    "prebuild": "npm-run-all --parallel gen:*",
    "prepackage": "npm run build && npm run test",
    "gen:config": "ts-json-schema-generator -p src/types/config.ts -t Config -o schemas/config.json -f tsconfig.json",
    "gen:template": "ts-json-schema-generator -p src/types/template.ts -t Template -o schemas/template.json -f tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1119.0",
    "jmespath": "^0.16.0",
    "jsonschema": "^1.4.0",
    "oclif": "^3.0.1",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jmespath": "^0.15.0",
    "@types/node": "^17.0.25",
    "cross-env": "^7.0.3",
    "nexe": "^4.0.0-beta.19",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.4",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "bin": {
    "clou": "./dist/cli/index.js",
    "sshls": "./infra/aws/bin/sshls",
    "scpls": "./infra/aws/bin/scpls"
  }
}
